{
  "name": "@slimevr/firmware-protocol-debugger-utils",
  "version": "0.0.7",
  "main": "dist/cjs/index.js",
  "types": "dist/dts/index.d.ts",
  "module": "dist/esm/index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "pnpm run build:esm && pnpm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "babel dist/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir dist/cjs --source-maps",
    "dev": "pnpm run build:esm -- -w",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@slimevr/common": "workspace:*",
    "@slimevr/firmware-protocol-debugger-shared": "workspace:*",
    "@slimevr/firmware-protocol": "workspace:*",
    "strict-event-emitter-types": "^2.0.0"
  },
  "devDependencies": {
    "@slimevr/tsconfig": "workspace:*",
    "@types/node": "^18.17.2"
  }
}
