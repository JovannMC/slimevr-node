{
  "name": "slimevr-firmware-protocol-debugger-gui",
  "version": "0.2.0",
  "main": "build/backend/electron/main.js",
  "license": "(MIT OR Apache-2.0)",
  "private": true,
  "homepage": ".",
  "author": {
    "name": "TheDevMinerTV",
    "email": "tobigames200@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SlimeVR/slimevr-node.git/tree/master/apps/firmware-protocol-debugger-gui"
  },
  "bugs": {
    "url": "https://github.com/SlimeVR/slimevr.ts/issues"
  },
  "dependencies": {
    "@react-three/fiber": "8.9.1",
    "@slimevr/common": "*",
    "@slimevr/firmware-protocol": "*",
    "@slimevr/firmware-protocol-debugger-utils": "*",
    "@slimevr/firmware-protocol-debugger-shared": "*",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "strict-event-emitter-types": "2.0.0",
    "three": "0.146.0",
    "ts-deepmerge": "5.0.0",
    "zustand": "4.1.4"
  },
  "devDependencies": {
    "@types/node": "17.0.45",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.6",
    "@types/three": "0.146.0",
    "@vitejs/plugin-react": "4.0.4",
    "autoprefixer": "10.4.13",
    "concurrently": "7.5.0",
    "cross-env": "7.0.3",
    "electron": "21.2.2",
    "electron-builder": "23.6.0",
    "electron-devtools-installer": "3.2.0",
    "postcss": "8.4.18",
    "tailwindcss": "3.2.2",
    "typescript": "4.8.4",
    "wait-for-it": "1.0.0",
    "vite": "4.4.8"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "dev": "concurrently --names 'be-tsc,fe,be-electron' \"tsc -w -p backend/tsconfig.json\" \"vite\" \"npm run dev:electron\"",
    "dev:electron": "cross-env ELECTRON_START_URL=http://localhost:5173 electron .",
    "build": "vite build && tsc -p backend/tsconfig.json && electron-builder --publish never",
    "dist": "electron-builder",
    "pack": "electron-builder --dir"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "dev.slimevr.firmware-protocol-debugger",
    "extends": null,
    "files": [
      "build/**/*",
      "dist/backend/**/*"
    ],
    "mac": {
      "category": "public.app-category.developer-tools"
    }
  }
}
